    $(document).ready(function() {
        var tbody = $(".table-html-check tbody");
        if (tbody[0].innerText == '') {
            $('.table-html-check').prev().remove();
        }

        $.each($('.show-success'), function( key, value ) {
            var text = $(this).text();
            text = text.replace(/\s/g, '');
            if (!text) {
                $(this).html(`<span><span class="badge badge-success">Successfully</span></span>`)
            }
        });

    });

    function goBack() {
        window.history.back();
    }

</script>