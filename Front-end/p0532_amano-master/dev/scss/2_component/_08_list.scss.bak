        box-sizing: border-box;
        opacity: 1 !important;
        position: relative;

        @include sp {
            width: 100%;
            padding: 25px 25px 79px;
        }

        &#{$root}--comming {
            background: #53646b;
            pointer-events: none;

            @include sp {
                display: none;
            }

            #{$root}__tit {
                color: #294050;
            }

            #{$root}__txt {
                color: #ffffff;
                position: absolute;
                top: calc(50% - 15px);
                left: 0;
                right: 0;
                text-align: center;
                @include montserrat;
                font-weight: bold;
                font-size: 1.8rem;
                letter-spacing: 0.9px;
            }
        }

        @include hover {
            #{$root}__bnt {
                border-color: #21587E;
                background: #ffffff;
                color: #265B7F;

                span {
                    &::before {
                        right: 15px;
                        background-image: url(/assets/img/common/icon/icon01_blue.svg);
                    }
                }
            }
        }

        @include sp {
            &+& {
                margin-top: 30px;
            }
        }
    }

    &__bnt {
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        background: #4686B2;
        text-align: center;
        color: #ffffff;
        font-size: 1.6rem;
        border: 1px solid #4686B2;
        font-weight: bold;
        transition: 300ms;
        height: 74px;
        display: flex;
        align-items: center;
        justify-content: center;

        @include sp {
            font-size: 1.5rem;
            padding-left: 20px;
            height: 60px;
        }

        span {
            @include sp {
                display: inline-block;
                padding-right: 40px;
                position: relative;
            }

            &::before {
                content: '';
                position: absolute;
                width: 20px;
                height: 13px;
                top: calc(50% - 6px);
                right: 20px;
                transition: 300ms;
                background: url(/assets/img/common/icon/icon01_white.svg);
                background-size: 20px 13px;

                @include sp {
                    right: 0;
                }
            }

        }
    }

    &__head {
        @include sp {
            @include fsc;
        }
    }

    &__tit {
        font-size: 2.2rem;
        color: #4686B2;
        text-align: center;

        @include sp {
            font-size: 1.8rem;
            line-height: 26px;
            flex: 1;
            text-align: left;
        }
    }

    img {
        margin: 18px 0 24px;
        width: 100%;

        @include sp {
            margin: 0;
            width: 55.7%;
        }
    }

    &__list {
        @include sp {
            margin-top: 20px;
        }

        li {
            font-size: 1.4rem;
            line-height: 23px;
            color: #111111;
            padding-left: 15px;
            position: relative;

            &::before {
                content: '';
                position: absolute;
                top: 9px;
                left: 0;
                width: 6px;
                height: 6px;
                background: #4686B2;
                border-radius: 100px;
            }

            &+li {
                margin-top: 9px;
            }
        }
    }
}

