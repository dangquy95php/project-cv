/*------------------------------------------------------------
list
------------------------------------------------------------*/

.c-list01 {
    display: flex;
    margin: 0 -10px;
    $root: &;

    @include sp {
        margin: 0;
        flex-wrap: wrap;
    }

    &__item {
        width: (100% / 3);
        padding: 0 10px;
        box-sizing: border-box;

        @include sp {
            width: 100%;
            padding: 0;
            margin-bottom: 15px;
        }
    }

    &__ttl {
        width: 100%;
        height: 100%;
        display: flex;
        justify-content: center;
        flex-direction: column;
        text-decoration: none;
        font-weight: 700;
        border: 3px solid $color4;
        padding: 26px 70px 26px 96px;
        box-sizing: border-box;
        position: relative;
        color: $color4;

        @include sp {
            padding: 15px 40px;
            border-width: 2px;
        }

        &:after,
        &:before {
            position: absolute;
            content: '';
            top: 50%;
            transform: translateY(-50%);
            margin: 1px 0 0;
        }

        &:after {
            position: absolute;
            content: '';
            right: 30px;
            width: 32px;
            height: 20px;
            background: url(/assets/img/common/icon/icon9_blue.svg) no-repeat center;
            background-size: 32px 20px;
            transition: all 0.3s;

            @include sp {
                right: 10px;
                width: 20px;
                height: 12px;
                background-size: 20px 12px;
            }
        }

        &:before {
            position: absolute;
            content: '';
            left: 40px;
            width: 26px;
            height: 48px;
            background: url(/assets/img/common/icon/icon_triangle.svg) no-repeat center;
            background-size: 26px 48px;

            @include sp {
                width: 16px;
                height: 38px;
                background-size: 16px 38px;
                left: 10px;
            }
        }

        @include hover {
            text-decoration: underline;
            opacity: 1;

            &:after {
                right: 25px;
            }

            #{$root}__big {
                color: $color4;
            }
        }
    }

    &__sm {
        font-size: 1.4rem;
        color: $color4;
    }

    &__big {
        font-size: 2.4rem;
        line-height: 1.5;
        color: $color1;
        margin: 1px 0 0;

        @include sp {
            font-size: 1.8rem;
        }
    }
}

