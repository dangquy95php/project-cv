.c-list02 {
    display: flex;
    flex-wrap: wrap;
    $root: &;

    //-------------------------------------------------------
    &__item {
        width: calc((100% - 62px) / 3);
        margin: 0 0 30px 31px;
        box-sizing: border-box;
        background-color: #fff;

        @include sp {
            width: 100%;
            margin: 0 0 20px;
        }

        &:nth-child(3n + 1) {
            margin: 0 0 30px;

            @include sp {
                margin: 0 0 20px;
            }
        }

        @include hover {
            cursor: pointer;

            #{$root}__img {
                &::before {
                    border-width: 20px 20px 0 20px;
                }
            }

            #{$root}__ttl {
                text-decoration: underline;
            }
        }
    }

    //-------------------------------------------------------
    &__img {
        position: relative;
        @include bf_hover;
        height: 250px;
        overflow: hidden;

        @include sp {
            height: auto;
        }

        img {
            width: 100%;
            height: 100%;
            object-fit: cover;

            @include sp {
                width: 100%;
                height: 100%;
                object-fit: cover;
            }
        }
    }

    //-------------------------------------------------------
    &__content {
        padding: 46px 17px 38px 32px;
        box-sizing: border-box;
        position: relative;

        @include sp {
            padding: 30px 20px 20px;
        }
    }

    //-------------------------------------------------------
    &__ttl {}

    //-------------------------------------------------------
    &__time {
        position: absolute;
        top: -14px;
        left: 20px;
        font-size: 1.3rem;
        color: #fff;
        line-height: 24px;
        display: flex;
    }

    .c-tag8 {
        position: absolute;
        top: 0;
        left: 0;
        z-index: 1;
    }

    //-------------------------------------------------------
    &--border {
        #{$root}__item {
            border: 1px solid #1c5472;
            box-sizing: border-box;
        }
    }

    //-------------------------------------------------------
    &.firstview {
        #{$root}__item {
            &:nth-child(3n + 1) {
                margin: 0 0 30px 31px;

                @include sp {
                    margin: 0 0 20px;
                }
            }

            &:nth-child(3n + 2) {
                margin: 0 0 30px;

                @include sp {
                    margin: 0 0 20px;
                }
            }

            &:last-child {
                @include sp {
                    margin: 0;
                }
            }

            &:hover {
                .c-list02__img::before {
                    border-width: 20px;
                }
            }

            &.firstview {
                width: 100%;
                margin: 0 0 30px;
                display: flex;
                flex-wrap: wrap;
                justify-content: space-between;

                @include sp {
                    border: none;
                    padding: 0;
                    margin: 0 0 20px;
                }

                #{$root}__img {
                    width: 50%;
                    height: 100%;

                    @include sp {
                        width: 100%;
                        height: auto;
                    }
                }

                #{$root}__content {
                    width: 50%;
                    padding: 60px;

                    @include sp {
                        width: 100%;
                        padding: 20px;
                    }
                }

                #{$root}__time {
                    position: relative;
                    top: auto;
                    left: auto;
                }

                #{$root}__ttl {
                    margin: 30px 0 0;
                    font-size: 2.6rem;
                    line-height: 1.5;

                    @include sp {
                        font-size: 1.6rem;
                        margin: 5px 0 0;
                    }
                }

                .c-tag8 {
                    font-size: 1.8rem;

                    @include sp {
                        font-size: 1.4rem;
                    }
                }

                .c-new__hastag {
                    margin: 29px 0 0;

                    @include sp {
                        margin: 10px 0 0;
                    }
                }
            }
        }
    }
}

